				<AudioClip ClipName="#@modfolder:Resources/Kaboomstick_sound.unity3d?kaboomstick_shoot05"/>
				<AudioClip ClipName="#@modfolder:Resources/Kaboomstick_sound.unity3d?kaboomstick_shoot06"/>
				<AudioClip ClipName="#@modfolder:Resources/Kaboomstick_sound.unity3d?kaboomstick_shoot07"/>
				<AudioClip ClipName="#@modfolder:Resources/Kaboomstick_sound.unity3d?kaboomstick_shoot08"/>
			<LocalCrouchVolumeScale value="1"/>
			<CrouchNoiseScale value="1"/>
			<NoiseScale value="1"/>
			<MaxVoices value="40"/>
			<MaxVoicesPerEntity value="10"/>
			<MaxRepeatRate value="0.001"/>
