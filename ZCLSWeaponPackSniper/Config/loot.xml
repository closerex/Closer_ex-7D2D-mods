<config>
    <conditional>
        <if cond="mod_loaded('EFTX_Pack_Core')">
			<append xpath="/lootcontainers/lootgroup[@name='groupWeaponsT2']">
				<item name="gunKF1894"/>
			</append>
			<append xpath="/lootcontainers/lootgroup[@name='groupWeaponsT3']">
				<item name="gunKFMosin"/>
				<item name="gunKFSPX464"/>
				<item name="gunTCTD2Virginian" loot_prob_template="veryLow"/>
			</append>
			<append xpath="/lootcontainers/lootgroup[@name='groupWeaponsT4']">
				<item name="gunKFM99"/>
			</append>
			<append xpath="//lootgroup[@name='groupQuestWeaponsRangedT2']">
				<item name="gunKF1894" loot_prob_template="QuestT2Prob"/>
				<item name="gunKFMosin" loot_prob_template="QuestT3Prob"/>
				<item name="gunKFSPX464" loot_prob_template="QuestT3Prob"/>
			</append>
			<append xpath="//lootgroup[@name='groupQuestWeaponsRangedT3']">
				<item name="gunKFMosin" loot_prob_template="QuestT3Prob"/>
				<item name="gunKFSPX464" loot_prob_template="QuestT3Prob"/>
				<item name="gunKFM99" loot_prob_template="QuestT4Prob"/>
				<item name="gunTCTD2Virginian" loot_prob_template="QuestT4Prob"/>
			</append>
			<append xpath="//lootgroup[@name='groupEftAmmoSmallT3']">
                <item name="ammoKF50BMGBulletBall" count="2,3" />
			</append>
			<append xpath="//lootgroup[@name='groupEftAmmoMediumT1']">
                <item name="ammoKF50BMGBulletBall" count="1,3" />
			</append>
			<append xpath="//lootgroup[@name='groupEftAmmoMediumT2']">
                <item name="ammoKF50BMGBulletBall" count="2,4" />
			</append>
			<append xpath="//lootgroup[@name='groupEftAmmoMediumT3']">
                <item name="ammoKF50BMGBulletBall" count="3,5" />
			</append>
			<append xpath="//lootgroup[@name='groupEftAmmoLargeT0']">
                <item name="ammoKF50BMGBulletBall" count="2,3" />
			</append>
			<append xpath="//lootgroup[@name='groupEftAmmoLargeT1']">
                <item name="ammoKF50BMGBulletBall" count="2,4" />
			</append>
			<append xpath="//lootgroup[@name='groupEftAmmoLargeT2']">
                <item name="ammoKF50BMGBulletBall" count="3,5" />
			</append>
			<append xpath="//lootgroup[@name='groupEftAmmoLargeT3']">
                <item name="ammoKF50BMGBulletBall" count="4,6" />
			</append>
            <append xpath="//lootgroup[@name='groupQuestAmmo']">
				<item name="ammoKF50BMGBulletBall" count="20,50" loot_prob_template="QuestT3Prob"/>
            </append>
        </if>
        <else>
			<insertAfter xpath="/lootcontainers/lootgroup[@name='empty']">
				<lootgroup name="groupWeaponsT2_DeadEyeKF1894" count="1" loot_quality_template="QLTemplateT2">
                    <item name="gunKF1894"/>
				</lootgroup>
				<lootgroup name="groupWeaponsT3_DeadEyeKFSPX464" count="1" loot_quality_template="QLTemplateT3">
                    <item name="gunKFSPX464"/>
				</lootgroup>
				<lootgroup name="groupWeaponsT3_DeadEyeKFMosin" count="1" loot_quality_template="QLTemplateT3">
                    <item name="gunKFMosin"/>
				</lootgroup>
				<lootgroup name="groupWeaponsT3_DeadEyeKFM99" count="1" loot_quality_template="QLTemplateT3">
                    <item name="gunKFM99"/>
				</lootgroup>
				<lootgroup name="groupWeaponsT3_DeadEyeTCTD2Virginian" count="1" loot_quality_template="QLTemplateT3">
                    <item name="gunTCTD2Virginian"/>
				</lootgroup>
                
                <lootgroup name="groupKF50BMGSmall">
                    <item name="ammoKF50BMGBulletBall" count="1,2" loot_stage_count_mod="0.01" loot_prob_template="ProbT3"/>
                </lootgroup>
                <lootgroup name="groupKF50BMGMedium">
                    <item name="ammoKF50BMGBulletBall" count="3,4" loot_stage_count_mod="0.01" loot_prob_template="ProbT3"/>
                </lootgroup>
                <lootgroup name="groupKF50BMGLarge">
                    <item name="ammoKF50BMGBulletBall" count="5,6" loot_stage_count_mod="0.01" loot_prob_template="ProbT3"/>
                </lootgroup>
            </insertAfter>
            
			<append xpath="/lootcontainers/lootgroup[@name='groupWeaponsT2_Ranged']">
                <item group="groupWeaponsT2_DeadEyeKF1894"/>
            </append>
            
			<append xpath="/lootcontainers/lootgroup[@name='groupWeaponsT3_Ranged']">
                <item group="groupWeaponsT3_DeadEyeKFSPX464" loot_prob_template="low"/>
                <item group="groupWeaponsT3_DeadEyeKFMosin" loot_prob_template="low"/>
                <item group="groupWeaponsT3_DeadEyeKFM99" loot_prob_template="low"/>
                <item group="groupWeaponsT3_DeadEyeTCTD2Virginian" loot_prob_template="veryLow"/>
            </append>

			<append xpath="/lootcontainers/lootgroup[@name='groupQuestWeaponsRangedT2']">
				<item name="gunKF1894"/>
			</append>

			<append xpath="/lootcontainers/lootgroup[@name='groupQuestWeaponsRangedT3']">
				<item name="gunKFSPX464" loot_prob_template="low"/>
				<item name="gunKFMosin" loot_prob_template="low"/>
				<item name="gunKFM99" loot_prob_template="low"/>
				<item name="gunTCTD2Virginian" loot_prob_template="veryLow"/>
			</append>

            <append xpath="/lootcontainers/lootgroup[@name='groupQuestAmmo']">
				<item name="ammoKF50BMGBulletBall" count="15" loot_prob_template="QuestT3Prob"/>
            </append>
            <append xpath="/lootcontainers/lootgroup[@name='groupAmmoSmall']">
				<item group="groupKF50BMGSmall"/>
            </append>
            <append xpath="/lootcontainers/lootgroup[@name='groupAmmoMedium']">
				<item group="groupKF50BMGMedium"/>
            </append>
            <append xpath="/lootcontainers/lootgroup[@name='groupAmmoSmallNoArrow']">
				<item group="groupKF50BMGSmall"/>
            </append>
            <append xpath="/lootcontainers/lootgroup[@name='groupAmmoMediumNoArrow']">
				<item group="groupKF50BMGMedium"/>
            </append>
            <append xpath="/lootcontainers/lootgroup[@name='groupAmmoLarge']">
				<item group="groupKF50BMGLarge"/>
            </append>
        </else>
    </conditional>
</config>