<configs>
	<conditional>
		<if cond="mod_loaded('EFTX_Pack_Standard')">
			<append xpath="/progression/crafting_skills/crafting_skill[@name='craftingRocketsandLaunchers']/display_entry[contains(@icon, 'gunchinalake')]/@icon">,gunKaboomstick,ammoKaboomstickShellHE,ammoKaboomstickShellHE</append>
			<append xpath="/progression/crafting_skills/crafting_skill[@name='craftingRocketsandLaunchers']/display_entry[contains(@icon, 'gunchinalake')]/unlock_entry[contains(@item, 'gunchinalake')]/@item">,gunKaboomstick,ammoKaboomstickShellHE,ammoKaboomstickShellHE</append>
			<append xpath="/progression/crafting_skills/crafting_skill[@name='craftingRocketsandLaunchers']/effect_group/passive_effect[contains(@tags, 'gunchinalake') and (@name='RecipeTagUnlocked' or @name='CraftingTier')]/@tags">,gunKaboomstick</append>
			<append xpath="/progression/crafting_skills/crafting_skill[@name='craftingRocketsandLaunchers']/effect_group/passive_effect[contains(@tags, 'gunchinalake') and @name='RecipeTagUnlocked']/@tags">,ammoKaboomstickShellHE,ammoKaboomstickShellHE</append>
		</if>
		<else>
			<!-- Kaboomstick Ammo -->
			<append xpath="/progression/crafting_skills/crafting_skill[@name='craftingExplosives']/display_entry[contains(@icon, 'ammoRocketFrag')]/unlock_entry/@item">,ammoKaboomstickShellFrag,ammoKaboomstickShellHE</append>
			<!-- Kaboomstick  -->
			<append xpath="/progression/crafting_skills/crafting_skill[@name='craftingExplosives']/display_entry[contains(@icon, 'gunExplosivesT3RocketLauncher')]/@icon">,gunKaboomstick</append>
			<append xpath="//crafting_skill[@name='craftingExplosives']/display_entry[contains(@icon, 'gunExplosivesT3RocketLauncher')]">
				<unlock_entry item="gunKaboomstick" unlock_tier="2" />
			</append>
			<append xpath="/progression/crafting_skills/crafting_skill[@name='craftingExplosives']/effect_group[1]">
				<passive_effect name="RecipeTagUnlocked" operation="base_set" level="45,100" value="1" tags="ammoKaboomstickShellFrag,ammoKaboomstickShellHE"/>
				<passive_effect name="RecipeTagUnlocked" operation="base_set" level="50,100" value="1" tags="gunKaboomstick"/>
				<passive_effect name="CraftingTier" operation="base_add" level="60,70,80,90,100" value="1,2,3,4,5" tags="gunKaboomstick"/>
			</append>
		</else>
	</conditional>
</configs>