<configs>
	<conditional>
		<if cond="mod_loaded('EFT_Overhaul')">
			<append xpath="/traders/trader_item_groups/trader_item_group[@name='groupGunsHugh']">
				<item name="gunKaboomstick"/>
			</append>
			<append xpath="/traders/trader_item_groups/trader_item_group[@name='groupModsBarney']">
				<item name="ammoKaboomstickShellHE" count="5,8"/>
				<item name="ammoKaboomstickShellFrag" count="5,8"/>
			</append>
			<append xpath="/traders/trader_item_groups/trader_item_group[@name='ammoExplosives']">
				<item name="ammoKaboomstickShellHE" count="1,2"/>
				<item name="ammoKaboomstickShellFrag" count="1,2"/>
			</append>
		</if>
		<if cond="mod_loaded('EFTX_Pack_Core')">
			<append xpath="/traders/trader_item_groups/trader_item_group[@name='ammoGroupSpecial']">
				<item name="ammoKaboomstickShellHE" count="25,50"/>
				<item name="ammoKaboomstickShellFrag" count="25,50"/>
			</append>
		</if>
		<else>
			<append xpath="/traders/trader_item_groups/trader_item_group[@name='groupRangedAll']">
				<item name="gunKaboomstick" quality="1,5"/>
			</append>
			<append xpath="/traders/trader_item_groups/trader_item_group[@name='ammoAll']">
				<item name="ammoKaboomstickShellHE" count="25,50"/>
				<item name="ammoKaboomstickShellFrag" count="25,50"/>
			</append>
			<append xpath="/traders/trader_item_groups/trader_item_group[@name='ammoHP']">
				<item name="ammoKaboomstickShellHE" count="25,50"/>
			</append>
			<append xpath="/traders/trader_item_groups/trader_item_group[@name='ammoAP']">
				<item name="ammoKaboomstickShellFrag" count="25,50"/>
			</append>
		</else>
	</conditional>
</configs>