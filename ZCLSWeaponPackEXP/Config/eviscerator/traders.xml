<configs>
	<conditional>
		<if cond="mod_loaded('EFT_Overhaul')">
			<append xpath="/traders/trader_item_groups/trader_item_group[@name='groupEftArcher' or @name='groupRangedAll']">
				<item name="meleeWpnBladeT3KF2Eviscerator"/>
			</append>
		</if>
		<else>
			<append xpath="/traders/trader_item_groups/trader_item_group[@name='groupRangedAll']">
				<item name="meleeWpnBladeT3KF2Eviscerator" quality="1,5"/>
			</append>
		</else>
	</conditional>
</configs>