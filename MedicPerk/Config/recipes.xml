		<if cond="mod_version('EFTX_Pack_Core') &gt;= version(2,5,2,5)">
			<setattribute xpath="/recipes/recipe[@name='gunMedicPistol']/ingredient[@name='resourceWeaponParts']" name="name">resourcePistolRepairKit</setattribute>
			<setattribute xpath="/recipes/recipe[@name='gunMedicSMG201']/ingredient[@name='resourceWeaponParts']" name="name">resourceSMGRepairKit</setattribute>
			<setattribute xpath="/recipes/recipe[@name='gunMedicShotgun301']/ingredient[@name='resourceWeaponParts']" name="name">resourceShotgunRepairKit</setattribute>
