<configs>
	<conditional>
		<if cond="mod_loaded('EFT_Overhaul')">
			<append xpath="/traders/trader_item_groups/trader_item_group[@name='groupGunsAll']">
				<item name="gunMedicPistol" quality="2,5"/>
				<item name="gunMedicSMG201" quality="2,5"/>
				<item name="gunMedicShotgun301" quality="2,5"/>
				<item name="gunMedicAssaultRifle401" quality="2,5"/>
			</append>
			<append xpath="/traders/trader_item_groups/trader_item_group[@name='groupGunsHugh']">
				<item name="gunMedicSMG201" quality="6"/>
				<item name="gunMedicShotgun301" quality="6"/>
				<item name="gunMedicAssaultRifle401" quality="6"/>
			</append>
			<append xpath="/traders/trader_item_groups/trader_item_group[@name='groupGunsBarney']">
				<item name="gunMedicShotgun301" quality="6"/>
				<item name="gunMedicAssaultRifle401" quality="6"/>
			</append>
		</if>
		<else>
			<append xpath="/traders/trader_item_groups/trader_item_group[@name='groupRangedAll']">
				<item name="gunMedicPistol" quality="2,5"/>
				<item name="gunMedicSMG201" quality="2,5"/>
				<item name="gunMedicShotgun301" quality="2,5"/>
				<item name="gunMedicAssaultRifle401" quality="2,5"/>
			</append>
			<append xpath="/traders/trader_item_groups/trader_item_group[@name='skillMagazines']">
					<item name="medicgunsSkillMagazine" count="1,3"/>	
			</append>
		</else>
	</conditional>
</configs>